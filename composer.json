{
    "name": "axlon/laravel-postal-code-validation",
    "description": "Worldwide postal code validation for Laravel and Lumen",
    "keywords": [
        "laravel",
        "lumen",
        "post code",
        "postal code",
        "zip code",
        "validation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "homepage": "https://github.com/axlon"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "illuminate/validation": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "brick/varexporter": "^0.3.2",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "symfony/console": "^4.3 || ^5.0",
        "symfony/http-client": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Axlon\\PostalCodeValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axlon\\PostalCodeValidation\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "PostalCode": "\\Axlon\\PostalCodeValidation\\Support\\Facades\\PostalCode"
            },
            "providers": [
                "\\Axlon\\PostalCodeValidation\\ValidationServiceProvider"
            ]
        }
    }
}
